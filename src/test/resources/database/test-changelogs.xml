<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.27.xsd">
    <changeSet id="prepare-local-postgresql-environment" author="skurelic">
        <sql>
            CREATE EXTENSION IF NOT EXISTS pg_trgm;
        </sql>
    </changeSet>

    <includeAll path="database/changelogs"/>

    <changeSet id="insert-test-data" author="skurelic" context="test">
        <sqlFile path="data.sql" relativeToChangelogFile="true"/>
    </changeSet>
</databaseChangeLog>